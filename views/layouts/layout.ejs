    <style>
        body {
            margin: 0 0 55px 0;
            background-color: #f1e5e8;
            font-family: "Noto Sans TC";
        }
        /* 修改 */
        
        .error {
            display: inline-block;
            padding: 1px 40px;
            font-size: 20px;
            cursor: pointer;
            text-align: center;
            text-decoration: none;
            outline: none;
            color: rgba(238, 5, 5, 0.705);
            background-color: rgba(255, 255, 255, 0.637);
            border: none;
            border-radius: 15px;
            box-shadow: 0 6px #999;
            transform: translateY(4px);
        }
        
        .mycard {
            display: inline-block;
            padding: 10px 40px;
            /* font-size: 40px; */
            cursor: pointer;
            text-align: center;
            text-decoration: none;
            outline: none;
            color: rgba(15, 8, 8, 0.705);
            background-color: rgb(255, 255, 255);
            border: none;
            border-radius: 15px;
            box-shadow: 0 6px #999;
            transform: translateY(4px);
        }
        
        .changebutton {
            display: inline-block;
            padding: 5px 40px;
            font-size: 35px;
            cursor: pointer;
            text-align: center;
            text-decoration: none;
            outline: none;
            color: #fff;
            background-color: rgb(255, 153, 0);
            border: none;
            border-radius: 15px;
            box-shadow: 0 6px #999;
        }
        
        .changebutton:hover {
            background-color: rgb(255, 153, 0)
        }
        
        .changebutton:active {
            background-color: #96710d;
            box-shadow: 0 5px #666;
            transform: translateY(4px);
        }
        /* 刪除 */
        
        .deletebutton {
            display: inline-block;
            padding: 5px 40px;
            font-size: 35px;
            cursor: pointer;
            text-align: center;
            text-decoration: none;
            outline: none;
            color: #fff;
            background-color: rgb(255, 17, 0);
            border: none;
            border-radius: 15px;
            box-shadow: 0 6px #999;
        }
        
        .deletebutton:hover {
            background-color: rgb(255, 17, 0)
        }
        
        .deletebutton:active {
            background-color: #96280d;
            box-shadow: 0 5px #666;
            transform: translateY(4px);
        }
        /* 預約 */
        
        .bookbutton {
            display: inline-block;
            padding: 5px 40px;
            font-size: 35px;
            cursor: pointer;
            text-align: end;
            text-decoration: none;
            outline: none;
            color: #fff;
            background-color: rgb(20, 141, 255);
            border: none;
            border-radius: 15px;
            box-shadow: 0 6px #999;
        }
        
        .bookbutton:hover {
            background-color: rgb(20, 141, 255)
        }
        
        .bookbutton:active {
            background-color: #154908;
            box-shadow: 0 5px #666;
            transform: translateY(4px);
        }
        /* 借取 */
        
        .borrowbutton {
            display: inline-block;
            padding: 5px 40px;
            font-size: 35px;
            cursor: pointer;
            text-align: center;
            text-decoration: none;
            outline: none;
            color: #fff;
            background-color: rgb(7, 168, 34);
            border: none;
            border-radius: 15px;
            box-shadow: 0 6px #999;
        }
        
        .borrowbutton:hover {
            background-color: rgb(7, 168, 34)
        }
        
        .borrowbutton:active {
            background-color: #154908;
            box-shadow: 0 5px #666;
            transform: translateY(4px);
        }
        
        .bottomnav {
            position: fixed;
            bottom: 0;
            width: 100%;
            height: 55px;
            box-shadow: 0 0 0px rgba(255, 255, 255, 0);
            background-color: #dee7ee;
            display: flex;
            overflow-x: auto;
        }
        
        .nav__link {
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            flex-grow: 1;
            min-width: 50px;
            overflow: hidden;
            white-space: nowrap;
            font-family: "Noto Sans TC";
            font-size: 15px;
            color: #444444;
            text-decoration: none;
            -webkit-tap-highlight-color: transparent;
            transition: background-color 0.1s ease-in-out;
        }
        /* .nav__link:hover {
      background-color: #eeeeee;
    } */
        
        .center {
            text-align: center;
            display: block;
            margin-left: auto;
            margin-right: auto;
            width: 90%;
        }
        
        .center2 {
            text-align: center;
            display: block;
            margin-left: 34px;
            margin-right: auto;
            width: 90%;
        }
        
        #outer {
            width: 100%;
            text-align: center;
        }
        
        table {
            border-collapse: collapse;
            width: 100%;
            display: inline-block;
        }
        
        table thead tr td {
            border-color: 1px solid red;
        }
        
        .black {
            border: 1px solid rgb(0, 0, 0);
        }
        
        .tr2 {
            border: 2px solid rgb(0, 0, 0);
            border-left: hidden;
            border-right: hidden;
        }
        
        table.table-bordered {
            border: 1px solid rgb(0, 0, 0);
            margin-top: 20px;
        }
        
        table.table-bordered>thead>tr>th {
            border: 2px solid rgb(0, 0, 0);
        }
        
        .square {
            width: 300px;
            height: 30px;
            color: rgb(41, 19, 19);
            background-color: rgb(255, 255, 255);
            border-radius: 4px;
        }
        
        .square2 {
            height: 40px;
            color: rgb(41, 19, 19);
            background-color: wheat;
            box-shadow: 0 5px brown;
            border-radius: 4px;
        }
        
        table.table-bordered>tbody>tr>td {
            border: 2px solid rgb(0, 0, 0);
        }
        
        .inner {
            display: inline-block;
            width: 40%;
        }
        
        .inner2 {
            display: inline-block;
            width: 40%;
        }
        
        .button {
            background-color: white;
            width: 200px;
            color: black;
            border: 2px solid rgb(0, 0, 0);
        }
        
        .center3 {
            text-align: center;
            display: block;
            margin-left: auto;
            margin-right: auto;
            width: 90%;
        }
        
        .responsive {
            width: 100%;
            height: auto;
        }
        
        .nav__icon {
            font-size: 18px;
        }
        
        #admintable {
            counter-reset: rowNumber;
        }
        
        #admintable th {
            counter-increment: rowNumber;
        }
        
        #admintable th:first-child::before {
            content: counter(rowNumber);
        }
        
        .class3 {
            background-color: lightsalmon
        }
        
        .admin {
            font-weight: bolder;
        }
        
        #adminestate {
            float: right;
        }
        
        #edit {
            color: blue;
            float: right;
        }
        
        #title {
            color: blue;
            font-weight: bolder;
            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
            text-align: justify;
        }
        
        #photo {
            float: left;
        }
        
        #info {
            text-align: center;
            display: inline;
        }
        
        #briefinfo {
            font-weight: bolder;
        }
        
        #dtitle {
            font-size: 50px;
            font-weight: bolder;
        }
        
        #detail {
            text-align: left;
        }
        
        #deleteid {
            display: none;
        }
    </style>

